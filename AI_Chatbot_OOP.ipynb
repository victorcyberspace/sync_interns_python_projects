        "# define the ChatbotModel class for managing chatbot functionality\n",
        "class ChatbotModel:\n",
        "\n",
        "  # initialize the model with paths for intents and directory\n",
        "  def __init__(self, intents_path, directory_path):\n",
        "\n",
        "    # store paths for intents and directory\n",
        "    self.intents_path = intents_path\n",
        "    self.directory_path = directory_path\n",
        "\n",
        "    # create a WordNetLemmatizer for word normalization\n",
        "    self.lemmatizer = WordNetLemmatizer()\n",
        "\n",
        "    # initialize empty lists for words, classes, and documents\n",
        "    self.words = []\n",
        "    self.classes = []\n",
        "    self.documents = []\n",
        "\n",
        "    # define punctuation characters to ignore\n",
        "    self.ignore_letters = ['?', '!', '.', ',']\n",
        "\n",
        "    # initialize empty lists for training data and model output\n",
        "    self.training = []\n",
        "    self.output_empty = []\n",
        "\n",
        "    # initialize variables for training data and model\n",
        "    self.train_x = None\n",
        "    self.train_y = None\n",
        "    self.model = None\n",
        "\n",
        "  # load intents from the specified JSON file\n",
        "  def load_intents(self):\n",
        "    with open(self.intents_path) as file:\n",
        "\n",
        "      # load intents from json\n",
        "      intents = json.load(file)\n",
        "    return intents\n",
        "\n",
        "  # preprocess data by extracting words, classes, and documents from intents\n",
